  "keywords": [
    "ai",
    "llm",
    "openai",
    "anthropic",
    "google",
    "gemini",
    "claude",
    "gpt",
    "gateway",
    "proxy",
    "routing",
    "fallback"
  ],
  "author": "Micah Nettey",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/micah-nettey/layer-ai.git",
    "directory": "packages/sdk"
  },
  "bugs": {
    "url": "https://github.com/micah-nettey/layer-ai/issues"
  },
  "homepage": "https://github.com/micah-nettey/layer-ai#readme",
  "dependencies": {
    "@layer-ai/types": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}