{
  "name": "@layer-ai/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for Layer AI - Manage LLM requests with intelligent routing and fallbacks",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
